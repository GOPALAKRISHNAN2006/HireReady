{
  "name": "hireready-backend",
  "version": "1.0.0",
  "description": "HireReady - AI-Powered Interview Preparation & Mock Interview Portal - Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seeds/seedData.js",
    "seed:challenges": "node seeds/seedCodingChallenges.js",
    "seed:all": "npm run seed && npm run seed:challenges",
    "test": "jest",
    "test:ci": "jest --coverage --runInBand",
    "lint": "eslint . --ext .js --max-warnings 0"
  },
  "keywords": [
    "interview",
    "ai",
    "mern",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "HireReady Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.4",
    "nodemailer": "^7.0.13",
    "openai": "^4.24.1",
    "socket.io": "^4.6.1",
    "stripe": "^14.25.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "swagger-ui-express": "^4.6.3",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
